<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Sequence for Repair Order -->
    <record id="seq_repair_order" model="ir.sequence">
        <field name="name">Repair Order Sequence</field>
        <field name="code">repair.order</field>
        <field name="prefix">REP</field>
        <field name="padding">5</field>
        <field name="number_increment">1</field>
        <field name="number_next">1</field>
    </record>

    <!-- Sequence for Device -->
    <record id="seq_repair_device" model="ir.sequence">
        <field name="name">Repair Device Sequence</field>
        <field name="code">repair.device</field>
        <field name="prefix">DEV</field>
        <field name="padding">5</field>
        <field name="number_increment">1</field>
        <field name="number_next">1</field>
    </record>

    <!-- Sequence for Appointment -->
    <record id="seq_repair_appointment" model="ir.sequence">
        <field name="name">Repair Appointment Sequence</field>
        <field name="code">repair.appointment</field>
        <field name="prefix">APT</field>
        <field name="padding">5</field>
        <field name="number_increment">1</field>
        <field name="number_next">1</field>
    </record>

</odoo>

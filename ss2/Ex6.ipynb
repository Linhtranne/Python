    "\n",
    "n = int(input(\"Nhập số nguyên dương n: \"))\n",
    "\n",
    "factorial = 1\n",
    "for i in range(1, n + 1):\n",
    "    factorial *= i\n",
    "\n",
    "print(f\"{n}! = {factorial}\")"

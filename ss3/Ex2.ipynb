    "def isPrime(n):\n",
    "    if n <= 1:\n",
    "        return print(\"điền số bình thường đi má\")\n",
    "    for i in range(2, int(n**0.5) + 1):\n",
    "        if n % i == 0:\n",
    "            return print(\"không phải số nguyên tố\")\n",
    "    return print(\"là số nguyên tố\")\n",
    "isPrime(int(input()))"
   ]
  }

   "source": [
    "def sum_of_digits(n):\n",
    "    total = 0\n",
    "    n = abs(n)\n",
    "    \n",
    "    while n > 0:\n",
    "        digit = n % 10 \n",
    "        total += digit\n",
    "        n = n // 10 \n",
    "    \n",
    "    return total\n",
    "\n",
    "n = int(input(\"Nhập n: \"))\n",
    "\n",
    "sum = sum_of_digits(n)\n",
    "print(f\"Tổng các chữ số của {n} là: {sum}\")"
   ]
  }

   "source": [
    "def circle_area(radius):\n",
    "    pi = 3.14159\n",
    "    area = pi * radius * radius\n",
    "    return area\n",
    "r = float(input())\n",
    "arena = circle_area(r)\n",
    "print(f\"Diện tích hình tròn là: {arena}\")"
   ]
